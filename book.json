{
    "root": "./",
    "title": "cheatsheet",
    "author": "XS Zhao",
    "plugins": [
        "prism", 
        "-highlight", 
        "addcssjs", 
        "anchor-navigation-ex", 
        "folding-chapters",
        "-lunr", "-search", "search-plus",
        "splitter",
        "sitemap",
        "github-buttons",
        "page-footer"
    ],
    "pluginsConfig": {
        "addcssjs": { 
            "js": [
                "./custom/my.js"
            ], 
            "css": [
                "./custom/my.css"
            ] 
        },
        "sitemap": {
            "hostname": "http://xszhao.science/cheatsheet"
        },
        "github-buttons": {
            "buttons": [{
                "user": "hansonzhao007",
                "repo": "cheatsheet",
                "type": "star",
                "size": "small",
                "count": true
                }   
            ]
        },
        "page-footer": {
            "description": "modified at",
            "signature": "XS Zhao",
            "wisdom": "Stay Foolish, Stay Hungry",
            "format": "yyyy-MM-dd hh:mm:ss",
            "copyright": "Copyright &#169; hansonzhao007",
            "timeColor": "#666",
            "copyrightColor": "#666",
            "utcOffset": "8",
            "isShowQRCode": true,
            "baseUri": "http://xszhao.science/cheatsheet/",
            "isShowIssues": true,
            "repo": "hansonzhao007/cheatsheet",
            "issueNum": "8",
            "token": "",
            "style": "normal"
        }
    }

}